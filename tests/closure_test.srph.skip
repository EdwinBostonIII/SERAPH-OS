fn apply(f: fn(i32) -> i32, x: i32) -> i32 {
    return f(x);
}

fn main() -> i32 {
    let double: fn(i32) -> i32 = |x: i32| -> i32 { return x * 2; };
    let triple: fn(i32) -> i32 = |x: i32| -> i32 { return x * 3; };

    let result1: i32 = apply(double, 5);
    let result2: i32 = apply(triple, 5);

    return result1 + result2;
}
