// Test enums with many variants

enum TokenType {
    TokEof,
    TokIdent,
    TokInt,
    TokFloat,
    TokString,
    TokIf,
    TokElse,
    TokWhile,
    TokFor,
    TokReturn,
    TokLet,
    TokFn,
    TokStruct,
}

fn token_is_keyword(t: TokenType) -> bool {
    if t == TokIf { return true; }
    if t == TokElse { return true; }
    return false;
}

fn main() -> i64 {
    let tok: TokenType = TokIf;
    if token_is_keyword(tok) {
        return 1;
    }
    return 0;
}
